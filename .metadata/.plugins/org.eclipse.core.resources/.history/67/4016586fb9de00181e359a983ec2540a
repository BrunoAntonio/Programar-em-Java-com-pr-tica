package application;

import entities.Pessoa;

public class Program {

	public static void main(String[] args) {
		
		
		Pessoa vendedor1 = new Pessoa();
		Pessoa vendedor2 = new Pessoa();
		
		vendedor1.setName("A");
		vendedor1.setPremioIndividual(50);
		vendedor1.setPremioEquipa(400);
		
		vendedor2.setName("B");
		vendedor2.setPremioIndividual(70);
		vendedor2.setPremioEquipa(500);
		
		System.out.println(vendedor1.toString());
		System.out.println(vendedor2.toString());
		
		}

}
