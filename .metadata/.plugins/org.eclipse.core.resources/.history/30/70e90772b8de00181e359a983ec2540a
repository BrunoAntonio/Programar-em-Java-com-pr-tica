package application;

import entities.Vendas;

public class Program {

	public static void main(String[] args) {
		
		
		Vendas vendedor1 = new Vendas();
		Vendas vendedor2 = new Vendas();
		
		vendedor1.setName("A");
		vendedor1.setPremioIndividual(50);
		vendedor1.setPremioEquipa(400);
		
		vendedor2.setName("B");
		vendedor2.setPremioIndividual(100);
		vendedor2.setPremioEquipa(500);
		
		System.out.println(vendedor1.toString());
		System.out.println(vendedor2.toString());
		
		}

}
