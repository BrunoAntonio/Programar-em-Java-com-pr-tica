package application;

import entities.Calculo;

public class Program {

	public static void main(String[] args) {
		
		double a=5;
		double b=4;
		
		Calculo calculo=new Calculo();
		
		;
		calculo.dividir(a, b);
		calculo.multiplicar(a, b);
		calculo.subtrair(a, b);
		
		System.out.printf("%.2f%n", calculo.somar(a, b));
		
		
		}

}
